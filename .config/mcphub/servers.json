{
  "nativeMCPServers": [],
  "mcpServers": {
    "openmemory": {
      "disabled": false,
      "url": "https://mem0.local.106source.ca/mcp/mcphub/sse/mike"
    },
    "taskmaster": {
      "disabled": false,
      "command": "task-master-ai",
      "env": {
        "ANTHROPIC_API_KEY": "$: cmd:pass show \"API/Claude API Key\"",
        "PERPLEXITY_API_KEY": "$: cmd:pass show \"API/Perplexity API Key\"",
        "MODEL": "claude-3-7-sonnet-20250219",
        "PERPLEXITY_MODEL": "sonar-pro",
        "MAX_TOKENS": 64000,
        "TEMPERATURE": 0.2,
        "DEFAULT_SUBTASKS": 5,
        "DEFAULT_PRIORITY": "medium"
      }
    },
    "github": {
      "disabled": false,
      "env": {
        "GITHUB_TOOLSETS": "repos,issues,pull_requests,code_security",
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      },
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "command": "docker"
    },
    "vectorcode": {
      "disabled": false,
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "args": [
        "--from",
        "vectorcode[mcp]",
        "--with",
        "mcp",
        "vectorcode-mcp-server"
      ],
      "command": "uvx"
    },
    "fetch": {
      "disabled": false,
      "args": ["mcp-server-fetch"],
      "command": "uvx"
    },
    "browsertools": {
      "command": "browser-tools-mcp"
    }
  }
}
