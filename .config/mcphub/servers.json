{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "fetch": {
      "disabled": false,
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "vectorcode": {
      "disabled": false,
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "command": "uvx",
      "args": [
        "--from",
        "vectorcode[mcp]",
        "--with",
        "mcp",
        "vectorcode-mcp-server"
      ]
    },
    "browserTools": {
      "command": "npx",
      "args": [
        "@agentdeskai/browser-tools-mcp@1.2.0"
      ]
    }
  }
}