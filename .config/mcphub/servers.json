{
  "nativeMCPServers": [],
  "mcpServers": {
    "openmemory": {
      "disabled": false,
      "url": "https://mem0.local.106source.ca/mcp/mcphub/sse/mike"
    },
    "taskmaster": {
      "disabled": false,
      "command": "task-master-ai"
    },
    "github": {
      "disabled": false,
      "env": {
        "GITHUB_TOOLSETS": "repos,issues,pull_requests,code_security",
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      },
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "command": "docker"
    },
    "vectorcode": {
      "disabled": false,
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "args": [
        "--from",
        "vectorcode[mcp]",
        "--with",
        "mcp",
        "vectorcode-mcp-server"
      ],
      "command": "uvx"
    },
    "fetch": {
      "disabled": false,
      "args": ["mcp-server-fetch"],
      "command": "uvx"
    },
    "browsertools": {
      "command": "browser-tools-mcp"
    }
  }
}
